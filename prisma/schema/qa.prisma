model Qa {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  title String
  body  String

  category   QaCategory? @relation(fields: [categoryID], references: [id], onDelete: SetNull)
  categoryID String?
}

model QaCategory {
  id    String @id @default(cuid())
  title String
  Qa    Qa[]
}
